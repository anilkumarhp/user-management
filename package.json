    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "node-cache": "^5.1.2",
    "nodemailer": "^7.0.3",
    "pg": "^8.16.0",
    "swagger-jsdoc": "^6.2.8",
